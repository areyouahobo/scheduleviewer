{
  "manifest_version": 2,
  "name": "Schedule Viewer",
  "version": "1.0.0",
  "author": "Wisor",
  "description": "A neato lil gadget so you know where to be",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
"browser_action": {
  "default_icon": "icon.png"
},
"background": {
  "scripts": ["background.js"]
}
}
